presentation of the text\n","X1 = np.zeros((len(df), 100))\n","for i, text in enumerate(df['lease']):\n","    vector = np.zeros(100)\n","    count = 0\n","    for word in text:\n","        if word in model.wv:\n","            vector += model.wv[word]\n","            count += 1\n","    if count != 0:\n","        vector /= count\n","    X1[i, :] = vector\n","\n","# Perform t-SNE to use the final provided number of columns in prediction by ML \n","tsne = TSNE(n_components=2, perplexity=40, learning_rate=200)\n","X1_tsne = tsne.fit_transform(X1)"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"n4vDOgajxMge"},"outputs":[],"source":["X1_df = pd.DataFrame(X1_tsne, columns = ['lease_1','lease_2'])\n","X1_df = X1_df.astype(float)"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"lpLBKj7iq74J"},"outputs":[],"source":["# Fit scaler on selected columns\n","scaler.fit(X1_df)\n","\n","# Transform selected columns\n","X1_df = scaler.transform(X1_df)\n","X1_df"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"OkO7G8euw4-M"},"outputs":[],"source":["# import module\n","#from sklearn.preprocessing import MinMaxScaler\n","\n","# scale features\n","#scaler = MinMaxScaler()\n","#model=scaler.fit(X1_df)\n","#X1_df =model.transform(X1_df)"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"aSaLCGUixeYa"},"outputs":[],"source":["X1_df = pd.DataFrame(X1_df, columns = ['lease_1','lease_2'])\n","X1_df = X1_df.round(decimals=2)\n","X1_df"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"b_-8lacsBcvv"},"outputs":[],"source":["df = pd.concat([df, X1_df], axis=1)\n","df"]},{"cell_type":"markdown","metadata":{"id":"1MmewPRWmiBj"},"source":["#Treating features containing location information of source as well as destination"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"background_save":true},"id":"3fSLo023yuvk"},"outputs":[{"data":{"text/plain":["\u003cmatplotlib.axes._subplots.AxesSubplot at 0x7f91a71dd640\u0